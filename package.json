{
	"name": "thesis-fe",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"format": "npx prettier --write \"src/**/*.svelte\""
	},
	"devDependencies": {
		"@material/typography": "^14.0.0",
		"@smui-extra/accordion": "^7.0.0-beta.14",
		"@smui-extra/badge": "^7.0.0-beta.14",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/card": "^7.0.0-beta.8",
		"@smui/common": "^7.0.0-beta.8",
		"@smui/layout-grid": "^7.0.0-beta.8",
		"@smui/list": "^7.0.0-beta.8",
		"@smui/menu-surface": "^7.0.0-beta.14",
		"@smui/paper": "^7.0.0-beta.8",
		"@smui/ripple": "^7.0.0-beta.8",
		"@smui/snackbar": "^7.0.0-beta.8",
